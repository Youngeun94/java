package Variables;
// \t : tab, \b : backspace, \f : form feed, \n : new line, \r : carriage return
// \' : 작은따음표, \" : 큰따옴표, \u유니코드 : 유티코드(16진수)문자 ex) char a='\u0041', \\ : 역슬래쉬
public class VariablesEx_09 {
	
	public static void main(String[] args) {
		System.out.println('\''); // '
		System.out.println("abc\t123\b456");// abc	12456
		System.out.println('\n');// (공백&줄바꿈)
		System.out.println("\"Hello\"");// "Hello"
		System.out.println("c:\\");// c:\
		
	}

}
